    "@bufbuild/buf": "^1.26.1",
    "@emotion/babel-plugin": "11.9.2",
    "@types/bcrypt": "^5.0.0",
    "@types/file-saver": "2.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.6.0",
    "@types/nodemailer": "^6.4.10",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "dotenv-cli": "7.3.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "prettier": "2.6.2",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "ts-proto": "1.110.4",
    "typescript": "5.1.6"
  }
