{
  "name": "sia-esec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "buf": "buf generate proto",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:withprisma": "prisma migrate deploy && npx prisma db seed && next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --write .",
    "docker": "docker buildx build --push --platform linux/amd64,linux/arm64 . --build-arg RPC_ADDRESS=host.docker.internal:4000 -t 780933574146.dkr.ecr.eu-west-3.amazonaws.com/esec-front:latest",
    "docker:export": "docker buildx build --load --platform linux/amd64 . --build-arg RPC_ADDRESS=host.docker.internal:4000 -t 780933574146.dkr.ecr.eu-west-3.amazonaws.com/esec-front:latest && docker save -o esec-front.tar 780933574146.dkr.ecr.eu-west-3.amazonaws.com/esec-front:latest"
  },
  "prisma": {
    "schema": "./src/lib/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/lib/prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@grpc/grpc-js": "1.6.7",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@react-pdf/renderer": "^3.1.12",
    "@reduxjs/toolkit": "1.8.1",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.9.8",
    "axios": "1.5.0",
    "bcrypt": "^5.1.1",
    "fast-sort": "3.1.3",
    "file-saver": "2.0.5",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "marked": "^9.1.1",
    "next": "12.1.5",
    "next-auth": "^4.23.1",
    "nodemailer": "^6.9.5",
    "nodemailer-brevo-transport": "^2.1.0",
    "normalize.css": "8.0.1",
    "react": "18.0.0",
    "react-chartjs-2": "4.1.0",
    "react-dom": "18.0.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.0",
    "react-redux": "8.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "select-files": "1.0.1"
  },
